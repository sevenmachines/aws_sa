\@writefile{lof}{\contentsline {figure}{\numberline {6}{\ignorespaces  Navigate to Services $\rightarrow $ EC2 $\rightarrow $ Security Groups. Select the \texttt  {ELBSecurityGroup} group and the 'Inbound' tab. By default, no inbound rules are set.\relax }}{7}}
\newlabel{fig:sg-elb_no_rules_full}{{6}{7}}
\@writefile{lof}{\contentsline {figure}{\numberline {7}{\ignorespaces Edit the group and add an inbound rule to allow http access from everywhere, $0.0.0.0/0$. Users will now be able to reach the load balancer from the internet and other external networks.\relax }}{7}}
\newlabel{fig:sg-elb_external_access}{{7}{7}}
\@writefile{toc}{\contentsline {paragraph}{Summary: }{8}}
\@writefile{toc}{\contentsline {paragraph}{Resolution: }{8}}
